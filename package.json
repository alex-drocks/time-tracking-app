{
  "name": "time-tracking",
  "version": "1.0.0",
  "description": "Web app for simple time tracking. Get amount of time spent on your tasks.",
  "scripts": {
    "start": "parcel --no-cache",
    "build": "rm -rf dist && parcel build --no-source-maps",
    "deploy": "firebase deploy"
  },
  "source": "src/index.html",
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 safari versions"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/alex-drocks/time-tracking-app.git"
  },
  "keywords": [
    "Time Tracking Web App"
  ],
  "author": "Alexandre Desroches",
  "license": "ISC",
  "devDependencies": {
    "parcel": "^2.0.0"
  },
  "dependencies": {
    "date-fns": "^2.25.0",
    "nanoid": "^3.1.30"
  }
}
